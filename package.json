{
  "name": "app2agent",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "pnpm -r dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "prepare": "[ \"$VERCEL\" = \"1\" ] || husky",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ]
  }
}
