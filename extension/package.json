{
  "name": "@app2agent/extension",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "dev:sidepanel": "cross-env VARIANT=sidepanel vite dev",
    "dev": "cross-env NODE_ENV=development concurrently \"cross-env vite build --watch --mode development -c vite.inject.config.ts\" \"node test/server.js\"",
    "build:sidepanel": "tsc && cross-env VARIANT=sidepanel vite build",
    "build:content": "tsc && cross-env VARIANT=content vite build",
    "build:inject": "tsc && vite build -c vite.inject.config.ts",
    "build": "pnpm run clean && pnpm run build:sidepanel && pnpm run build:inject",
    "lint": "eslint .",
    "preview": "vite preview",
    "release": "pnpm run build && node -e \"const AdmZip = require('adm-zip'); const zip = new AdmZip(); zip.addLocalFolder('./dist/sidepanel'); zip.addLocalFolder('./dist/sidepanel'); zip.writeZip('./dist/extension.zip');\""
  },
  "dependencies": {
    "@google/genai": "^1.7.0",
    "@google/generative-ai": "^0.21.0",
    "@llamaindex/core": "^0.6.12",
    "@llamaindex/google": "^0.3.11",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@types/rollup": "^0.54.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eventemitter3": "^5.0.1",
    "html2canvas": "^1.4.1",
    "llamaindex": "^0.11.11",
    "lodash": "^4.17.21",
    "lucide-react": "^0.469.0",
    "pathe": "^1.1.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^9.1.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/chrome": "^0.0.254",
    "@types/lodash": "^4.17.19",
    "@types/node": "^20.19.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "adm-zip": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "express": "^4.21.2",
    "globals": "^15.15.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tailwindcss-scoped-preflight": "^3.4.12",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^5.4.19",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-plugin-web-extension": "^4.4.4"
  }
}
