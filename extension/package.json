{
  "name": "@app2agent/extension",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "VARIANT=docs vite dev",
    "build:docs": "tsc && VARIANT=docs vite build",
    "build": "pnpm run clean && pnpm run build:docs",
    "lint": "eslint .",
    "preview": "vite preview",
    "release": "pnpm run build && node -e \"const AdmZip = require('adm-zip'); const zip = new AdmZip(); zip.addLocalFolder('./dist/docs'); zip.writeZip('./dist/extension.zip');\""
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "eventemitter3": "^5.0.1",
    "lodash": "^4.17.21",
    "pathe": "^1.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/chrome": "^0.0.254",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20.17.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "adm-zip": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "tailwindcss-scoped-preflight": "^3.4.10",
    "tslib": "^2.8.1",
    "typescript": "^5",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vite-plugin-web-extension": "^4.3.1"
  }
}
